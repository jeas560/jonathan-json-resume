{
  "name": "jonathan-json-resume",
  "version": "1.0.0",
  "description": "My resume in the [JSON Resume](https://jsonresume.org/) format. Built and previewed with `resume-cli`.",
  "main": "index.js",
  "scripts": {
    "init": "resumed init resume.json",
    "validate": "resumed validate resume.json",
    "html": "mkdir -p dist && resumed render resume.json -o dist/index.html",
    "pdf": "node scripts/export-pdf.mjs",
    "preview": "npm run html && npx http-server dist -p 4000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeas560/jonathan-json-resume.git"
  },
