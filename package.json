  },
  "homepage": "https://github.com/jeas560/jonathan-json-resume#readme",
  "devDependencies": {
    "jsonresume-theme-even": "^0.23.0",
    "puppeteer": "^24.16.1",
    "resumed": "^6.0.0"
  },
  "dependencies": {
    "character-entities": "^1.2.4",
    "character-entities-legacy": "^1.1.4",
    "character-reference-invalid": "^1.1.4",
    "classnames": "^2.5.1",
    "core-js": "^3.45.0",
    "debug": "^4.4.1",
    "feather-icons": "^4.29.2",
    "is-alphabetical": "^1.0.4",
    "is-alphanumerical": "^1.0.4",
    "is-decimal": "^1.0.4",
    "is-hexadecimal": "^1.0.4",
    "jsonschema": "^1.5.0",
    "micromark": "^2.11.4",
    "mri": "^1.2.0",
    "ms": "^2.1.3",
    "parse-entities": "^2.0.0",
    "sade": "^1.8.1",
    "strip-json-comments": "^5.0.3",
    "striptags": "^3.2.0"
  }
}
